{
  "name": "study_with_ia",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "prepare": "cypress install",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test 'vite preview --port 4173' http://localhost:4173 'cypress run --e2e'",
    "tests": "npm run test:unit && npm run test:e2e:ci",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "pipeline:real:full": "npm run install && npm run lint && npm run tests && npm run build && npm run simulate:deploy",
    "simulate:install": "node src/scripts/simulate-install.js",
    "simulate:lint": "node src/scripts/simulate-lint.js",
    "simulate:tests": "node src/scripts/simulate-tests.js",
    "simulate:e2e": "node src/scripts/simulate-e2e.js",
    "simulate:build": "node src/scripts/simulate-build.js",
    "simulate:deploy": "node src/scripts/simulate-deploy.js",
    "pipeline:simulate:full": "npm run simulate:install && npm run simulate:lint && npm run simulate:tests && npm run simulate:build && npm run simulate:deploy"
  },
  "dependencies": {
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.17.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.5.3",
    "eslint": "^9.31.0",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "start-server-and-test": "^2.0.12",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
  }
}
